generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["prismaSchemaFolder", "driverAdapters"]
  output          = "../../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Article {
  id          Int      @id @default(autoincrement())
  numero      Int
  createdAt   DateTime @default(now())
}
